@using Melidya.ENTITY
@model  List<Orders>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>#</th>
            <th>Contact Name</th>
            <th>Freight</th>
            <th>Employee First Name</th>
            <th>Order Date</th>
            <th>Detay</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Orders item in Model)
        {
        <tr>
            <th scope="row">@item.OrderID</th>
            <td>@item.Customers.ContactName</td>
            <td>@item.Freight</td>
            <td>@item.Employees.FirstName</td>
            <td>@item.OrderDate</td>
            <td><a href="/Order/GetDetails/@item.OrderID">Detail</a></td>

        </tr>
        }
    </tbody>
</table>